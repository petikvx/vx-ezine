Program Virus_Sort;

uses dos;

var log_file:text;
    virus_string:string;




Function DosShell(command:String):Integer;Var
 OldHeapEnd,
 NewHeapEnd: Word;
 Error:Integer;
Begin
 Error:=0;
 If MemAvail<$1000 then Error:=8;
 If Error=0 then Begin
  NewHeapEnd:=Seg(HeapPtr^)-PrefixSeg;
  OldHeapEnd:=Seg(HeapEnd^)-PrefixSeg;
   asm
    mov ah,4Ah
    mov bx,NewHeapEnd
    mov es,PrefixSeg
    Int 21h
    jnc @EXIT
    mov Error,ax
    @EXIT:
   end; {asm}
  If Error=0 then begin
   SwapVectors;
   Exec(GetEnv('COMSPEC'),command);
   SwapVectors;
    asm
     mov ah,4Ah
     mov bx,OldHeapEnd
     mov es,PrefixSeg
     Int 21h
     jnc @EXIT
     mov Error,ax
     @EXIT:
    end; {asm}
  end;   {If}
 end;    {If}
 DosShell:=Error;
end;     {Function}




procedure copy_name(x:string);
var counter:integer;
    counter2:integer;
    execute_n:string;
    zip_n:string;

    top_diz:string;
    description:string;
    bottom_diz:string;

    diz:text;

begin
 Counter:=0;
 While X[Counter]<>' ' do Counter:=Counter+1;

 zip_n:=copy(X,1,Counter-5);
 execute_n:=copy(X,1,Counter);

 description:='³'+copy(X,Counter,Length(X))+'  ³';
 assign(diz,'FILE_ID.DIZ');
 rewrite(diz);


 {==============}
 top_diz:='Õ';
 counter2:=0;
 for counter2:=1 to length(description)-2 do top_diz:=top_diz+'Í';
 top_diz:=top_diz+'¸';
 {=====================}
 bottom_diz:='Ô';
 counter2:=0;
 for counter2:=1 to length(description)-2 do bottom_diz:=bottom_diz+'Í';
 bottom_diz:=bottom_diz+'¾';


 writeln(diz,top_diz);
 writeln(diz,description);
 writeln(diz,bottom_diz);

 writeln(top_diz);
 writeln(description);
 writeln(bottom_diz);

 close(diz);
 DosShell('/C PKZIP '+zip_n+' FILE_ID.DIZ '+execute_n+' >NUL');
end;


begin
Writeln('Nem Sort v1.0');
if paramcount<>1 then begin
Writeln('This is NemSort v1.0 - The First and Probably Last Revision');
Writeln('It was written in 4 minutes using TP7.0');
Writeln('You Forgot to Specify the Log File Name!!!');
halt;
end;


assign(log_file,paramstr(1));
Reset(log_file);


repeat
readln(log_file,virus_string);

copy_name(virus_string);

until eof(log_file);
close(log_file);


end.

