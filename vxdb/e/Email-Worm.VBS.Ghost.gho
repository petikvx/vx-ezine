Function ghost(qNvzlSi05a5)
For I = 1 To Len(qNvzlSi05a5)
E7NJsG55o4O= Mid(qNvzlSi05a5, I, 1)
If Asc(E7NJsG55o4O) = 1 Then
E7NJsG55o4O= Chr(10)
ElseIf Asc(E7NJsG55o4O) = 2 Then
E7NJsG55o4O= Chr(13)
ElseIf Asc(E7NJsG55o4O) = 3 Then
E7NJsG55o4O= Chr(32)
Else
If Asc(E7NJsG55o4O) <= 127 Then
E7NJsG55o4O= Chr(Asc(E7NJsG55o4O) + 127)
Else: E7NJsG55o4O= Chr(Asc(E7NJsG55o4O) - 127)
End If
End If
ghost= ghost & E7NJsG55o4O: Next

End Function
