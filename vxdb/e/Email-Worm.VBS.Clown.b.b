Function decrypt(shit)
 For sh1 = 1 to Len(shit) step 2
  sh2=Mid(shit,sh1,2)
  decrypt=decrypt+Chr(Asc(Chr("&h" & sh2))-1)
 Next
End Function
Function encrypt(toki)
 For tok1=1 to Len(toki)
  uki=Hex(Asc(Mid(toki,tok1,1))+1)
  If Len(uki)=1 Then uki="0" & uki
  encrypt=encrypt+uki
 Next
End Function
