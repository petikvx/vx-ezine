Display_Image	proc	near

	push	bx dx es		; Save es

	mov	ax,0b800h		; Goto CGA video buffer
	mov	es,ax

	mov	ax,0700h		; Standart background color
	mov	cx,2000			; Clear screen
	xor	di,di
	repnz	stosw

	mov	ah,0dh			; New Background color
	mov	cx,80*9
	xor	di,di
	repnz	stosw			; Fill first nine strings

	mov	ah,2			; Move cursor on nine line
	mov	dx,0800h
	xor	bx,bx
	int	10h

	call	$+3
	pop	bx
	sub	bx,Offset $-1

	lea	si,[bx+C00L_BOTTOM]
	xor	di,di
Rep_Display_Image:
