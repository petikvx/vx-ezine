Function IsMail(ML)
On error resume next
Dim R(13)
R(0) = "/": R(1) = "\": R(2) = "?": R(3) = "="
R(4) = ">"
R(5) = "<": R(6) = Chr(34): R(7) = ";": R(8) = ","
R(9) = Chr(37): R(10) = "¡": R(11) = "¿": R(12) = ")"
R(13) = "("
For K = 0 To 13
If InStr(ML, R(K)) <> 0 Then
IsMail = False
Exit Function
End If
Next
X1 = InStr(ML, "@"): X2 = InStr(ML, ".")
If (X1 <> 0) And (X2 <> 0) And (X1 < X2) Then
IsMail = True
End If
End Function
