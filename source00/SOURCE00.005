						;  get present dir,drive dl,1=a:
						;   ds:si=ASCIIZ directory name
		pop	ax
		mov	ah,0Eh
		sub	al,1
		mov	dl,al
		int	21h			; DOS Services  ah=function 0Eh
						;  set default drive dl  (0=a:)
		mov	es:data_7,al
loc_1:
		mov	al,byte ptr es:[249h]
		cmp	al,0
