.model tiny
.code
.386
.startup
          mov   dx,offset msg
          call  print

          sub   ax,ax
          sub   cx,cx
          mov   ds,ax
          lds   si,ds:[1*4]
          mov   ax,0cf41h ; inc cx/iret
          xchg  ax,ds:[si]

          pushf           ; save flags
          pushf
          pop   bx
          or    bh,1      ; Set trace flag on
          push  bx
          popf

          push  bx        ;*
          popf            ;*
          inc   cx        ;*

          popf            ; restore flags

          xchg  ax,ds:[si];restore vector

          push  cs
          pop   ds
          mov   dx,offset nodebug

          cmp   cx,5
          je    print
          mov   dx,offset debug
print:    mov   ah,9
          int   21h
          retn
msg       db    'Fuck debug (c) 2:5050/36.19',13,10,'$'
debug     db    'hi debugger;)',13,10,'$'
nodebug   db    'all ok',13,10,'$'
end
