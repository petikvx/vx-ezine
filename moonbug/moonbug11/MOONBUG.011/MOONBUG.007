          sub  cx,cx
          mov  bp,sp
          mov  2 ptr[bp-2],offset $retn
          call [bp-2]
          inc  cx
$retn:    retn
allok     db 'All Ok',0dh,0ah,'$'
emulator  db 'hi emulator',0dh,0ah,'$'
