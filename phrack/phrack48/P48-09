LOCATE 8,2
MESSAGE "LOCAL LOCATION CLLI CODE ="
LOCATE 8,29
MESSAGE S2
LOCATE 10,2
MESSAGE "SYSTEM ID = "
LOCATE 10,17
MESSAGE S8
LOCATE 12,2
MESSAGE "SYSTEM NUMBER = "
LOCATE 12,21
MESSAGE S9
LOCATE 17,2
MESSAGE "IS INFORMATION CORRECT?  Y/N + <ENTER>"
LOCATE 17,44
GET S8 1
SWITCH S8
     CASE "Y"
          ;DO NOTHING
     ENDCASE
     DEFAULT
          GOTO LABEL1 ; JUMP TO TOP AND ENTER INFORMATION AGAIN
     ENDCASE
ENDSWITCH
CLEAR
LOCATE 8,15
MESSAGE "DO NOT PRESS ANY KEYS UNTIL CONFIGURATION COMPLETE"
LOCATE 10,15
MESSAGE "OK...HERE WE GO..."
ALARM 1
PAUSE 2

;SET DATE DP3024
TRANSMIT "CD"
TRANSMIT S3
TRANSMIT " "
TRANSMIT S4
TRANSMIT " "
TRANSMIT S5
TRANSMIT " !"
