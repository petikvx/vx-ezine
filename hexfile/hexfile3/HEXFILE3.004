                 dec     ax
                 mov     cl,4
                 shr     ax,cl
                 add     ax,dx
                 cwd
                 div     word ptr [bx].SectorPerTrack
                 mov     cl,dl
                 cwd
                 div     word ptr [bx].NumberSides
                 inc     cx
                 mov     ch,al
                 mov     dh,dl
                 mov     dl,0
                 mov     bp,3
 
